{
  "manifest_version": 3,
  "name": "My Chrome Extension",
  "version": "0.1.0",
  "description": "My first Chrome extension.",
  "action": {
    "default_popup": "./static/popup.html"
  },
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./dist/content.js"]
    }
  ],
  "background": {
    "service_worker": "./dist/background.js"
  }
}
